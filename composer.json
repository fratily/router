{
    "name": "fratily/router",
    "description": "",
    "type": "library",
    "keywords": ["fratily", "router", "routing"],
    "authors": [
        {
            "name": "Kento Oka",
            "email": "kento-oka@kentoka.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fratily\\Router\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Fratily\\Tests\\Router\\": "tests/"
        }
    },
    "scripts": {
        "phpcs":"phpcs --standard=PSR12 src",
        "phpcbf":"phpcbf --standard=PSR12 src",
        "phpcs-tests":"phpcs --standard=PSR12 tests",
        "phpcbf-tests":"phpcbf --standard=PSR12 tests"
    }
}
